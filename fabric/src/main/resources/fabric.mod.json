{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_website}",
    "sources": "${mod_website}",
    "issues": "${mod_website}/issues"
  },
  "license": "${mod_license}",
  "icon": "assets/better_slots/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.alfakynz.better_slots.fabric.ExampleModFabric"
    ],
    "client": [
      "com.alfakynz.better_slots.fabric.client.ExampleModFabricClient"
    ]
  },
  "mixins": [
    "better_slots.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=17"
  }
}
